{
    "buildCommand": "py manage.py collectstatic",
    "builds": [{
        "src": "api_taller/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api_taller/wsgi.py"
        }
    ]
   
}
